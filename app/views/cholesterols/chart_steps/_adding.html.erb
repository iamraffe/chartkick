  <h2>Please add the cholesterol data for: <%= @chart.name %></h2>
  <h3>Make sure to include the date for each test.</h3>
  <table class="table table-striped table-hover cholesterol-data">
    <thead>
      <tr>
        <th>#</th>
        <th>Date</th>
        <th>LDL</th>
        <th>HDL</th>
        <th>Triglycerides</th>
      </tr>
    </thead>
    <tbody>
      <% 5.times do |i| %>
        <tr>
          <td><%= (i+1) %></td>
          <td><input type="date" name="entry[date[<%= (i+1) %>]]"></td>
          <td><input type="text" name="entry[ldl[<%= (i+1) %>]]"></td>
          <td><input type="text" name="entry[hdl[<%= (i+1) %>]]"></td>
          <td><input type="text" name="entry[triglycerides[<%= (i+1) %>]]"></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <button class="random btn btn-primary" style="margin-bottom: 25px;">Generate random numbers</button>
  <button class="add-row btn btn-primary pull-right" style="margin-bottom: 25px;">Add row</button>
