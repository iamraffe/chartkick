  <h2>Please add the cholesterol data for: <%= @chart.name %></h2>
  <h3>Make sure to include the date for each test.</h3>
  <table class="table table-striped table-hover ">
    <thead>
      <tr>
        <th>#</th>
        <th>Date</th>
        <th>LDL</th>
        <th>HDL</th>
        <th>Triglycerides</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td>1</td>
        <td><input type="date" name="entry[date[0]]"></td>
        <td><input type="text" name="entry[ldl[0]]"></td>
        <td><input type="text" name="entry[hdl[0]]"></td>
        <td><input type="text" name="entry[triglycerides[0]]"></td>
      </tr>
      <tr>
        <td>2</td>
        <td><input type="date" name="entry[date[1]]"></td>
        <td><input type="text" name="entry[ldl[1]]"></td>
        <td><input type="text" name="entry[hdl[1]]"></td>
        <td><input type="text" name="entry[triglycerides[1]]"></td>
      </tr>
      <tr class="">
        <td>3</td>
        <td><input type="date" name="entry[date[2]]"></td>
        <td><input type="text" name="entry[ldl[2]]"></td>
        <td><input type="text" name="entry[hdl[2]]"></td>
        <td><input type="text" name="entry[triglycerides[2]]"></td>
      </tr>
      <tr class="">
        <td>4</td>
        <td><input type="date" name="entry[date[3]]"></td>
        <td><input type="text" name="entry[ldl[3]]"></td>
        <td><input type="text" name="entry[hdl[3]]"></td>
        <td><input type="text" name="entry[triglycerides[3]]"></td>
      </tr>
    </tbody>
  </table>
  <button class="random btn btn-primary" style="margin-bottom: 25px;">Generate random numbers</button>
