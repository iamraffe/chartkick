<div class="container">
  <div class="row">
    <h1>Patient name:  <span class="patient-name"><%= @chart.name %></span></h1>
  </div>
    <%= line_chart construct_cholesterol_chart_from_database(@chart.entries), id: "users-chart", height: "500px", discrete: true, library: {curveType: "none", pointSize: 5, crosshair: { trigger: 'both' }, colors: ['#111A33', '#001E93', '#4FCFEB', '#A725A7']} %>
  </div>
  <div class="row">
    <div class="col-xs-8 col-xs-offset-2">
      <%= link_to "Back", root_path, class: "btn btn-primary btn-lg " %>
      <%= link_to "View as PDF", params.merge(:format => :pdf), class: "btn btn-success btn-lg pull-right", target: '_blank' %>
    </div>
  </div>
</div>
