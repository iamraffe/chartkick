<section class="patients">
  <section class="col-sm-6">
    <ul class="list-unstyled my-patients">
      <% @users.each do |user| %>
        <li class="patient">
          <%= link_to user_path(user) do %>
            <img src="<%= user.avatar %>" alt="<%= user.full_name %>" class=" img-circle" style="">
            <span class="patient-name"><%= "#{user.last_name}, #{user.first_name}" %></span>
          <% end %>
        </li>
      <% end %>
    </ul>

  </section>
  <section class="col-sm-6 patient-actions">

  </section>
</section>

